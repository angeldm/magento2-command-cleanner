{
    "name": "angeldm/magento2-command-cleanner",
    "type": "magento2-module",
    "description": "agento 2 module to clean directories pub and var",
    "keywords": [
        "composer-installer",
        "magento",
        "Magento 2",
        "Angeldm"
    ],
    "homepage": "https://github.com/angeldm/magento2-command-cleanner",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Àngel Díaz",
            "email": "angeldm@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "magento/framework": "102.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "magento/marketplace-eqp": "~2.0.0",
        "phpmd/phpmd": "~2.0",
        "sebastian/phpcpd": "3.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Angeldm\\Cleanner\\": ""
        },
        "files": [
            "registration.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}